import java.util.HashMap;
import java.util.Set;


/* Десять котов
Создать класс кот – Cat, с полем «имя» (String).
Создать словарь Map(<String, Cat>) и добавить туда 10 котов в виде «Имя»-«Кот».
Получить из Map множество(Set) всех имен и вывести его на экран.
*/

public class Main
{
    public static void main(String[] args)
    {
        HashMap<String, Cat> map = createMap();
        Set<String> set = map.keySet();
        printCatSet(set);
    }

    public static HashMap<String, Cat> createMap()
    {
        HashMap<String, Cat> map = new HashMap<>();

        for (int i = 0; i < 10; i++)
        {
            String name = Integer.toOctalString(i);
            map.put(name, new Cat(name));
        }
        return map;
    }

    public static void printCatSet(Set<String> set)
    {
        for (String catName:set)
        {
            System.out.println(catName);
        }
    }

    public static class Cat
    {
        private String name;

        public Cat(String name)
        {
            this.name = name;
        }

        public String toString()
        {
            return "Cat "+this.name;
        }
    }
}
